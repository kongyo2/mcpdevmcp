{
    "plugins": [
        "promise",
        "import",
        "node"
    ],
    "categories": {
        "correctness": "error",
        "suspicious": "warn"
    },
    "rules": {
        "no-console": "off",
        "typescript/no-explicit-any": "error"
    },
    "ignorePatterns": [
        "node_modules",
        "dist",
        "build",
        "coverage",
        "*.min.js"
    ]
}